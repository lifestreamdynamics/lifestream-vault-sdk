{
  "name": "@lifestreamdynamics/vault-sdk",
  "version": "1.0.1",
  "description": "Official TypeScript SDK for Lifestream Vault API",
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/__tests__",
    "README.md"
  ],
  "keywords": [
    "lifestream",
    "vault",
    "markdown",
    "webdav",
    "sdk",
    "api-client",
    "typescript"
  ],
  "author": "Lifestream Dynamics <eric@lifestreamdynamics.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lifestreamdynamics/lifestream-vault-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/lifestreamdynamics/lifestream-vault-sdk/issues"
  },
  "homepage": "https://github.com/lifestreamdynamics/lifestream-vault-sdk#readme",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prepublishOnly": "npm run build && npm test"
  },
  "dependencies": {
    "ky": "^1.8.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "typescript": "^5.7",
    "vitest": "^4.0.18"
  }
}
